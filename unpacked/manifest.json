{
	"name": "前端助手",
	"version": "1.2.9",
	"description": "协助前端快速获取整站资源",
	"icons": {
    "128": "icon.png"
	},
	"browser_action": {
    "default_icon": "icon.png",
		"default_title": "UP",
		"default_popup": "popup.html"
  },
	"background": {
		"scripts": [
			"background.js"
		]
  },
	"permissions": [
		"tabs",
		"http://*/",
    "https://*/",
		"*://*/*",
		"downloads",
		"downloads.shelf"
	],
	"devtools_page": "devtool.html",
	"manifest_version": 2
}
